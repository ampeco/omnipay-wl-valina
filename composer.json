{
    "name": "ampeco/omnipay-wl-valina",
    "description": "Omnipay plugin for Worldline Valina",
    "version": "1.5.4",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ampeco\\OmnipayWlValina\\": "src/",
            "Ampeco\\OmnipayWlValina\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Valentin Damyanov",
            "email": "valentin.damyanov@ampeco.global"
        },
        {
            "name": "Bozhidar Gyurov",
            "email": "bozhidar.gyurov@ampeco.global"
        }
    ],
    "require": {
        "laravel/framework": "^10.0|^11.0",
        "omnipay/common": "^3.2",
        "php": "^8.1",
        "nyholm/psr7": "^1.0"
    },
    "allow-plugins": {
        "php-http/discovery": true
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
